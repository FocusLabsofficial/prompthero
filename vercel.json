{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "docs/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "docs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/",
      "dest": "/docs/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}